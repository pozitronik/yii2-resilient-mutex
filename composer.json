{
  "name": "beeline/yii2-resilient-mutex",
  "description": "Fault-tolerant mutex with circuit breaker pattern and automatic backend switching for Yii2",
  "type": "yii2-extension",
  "keywords": ["yii2", "mutex", "resilience", "circuit-breaker", "fault-tolerance", "high-availability"],
  "license": "LGPL-3.0-or-later",
  "authors": [
		{
			"name" : "Pavel Dubrovsky",
			"email": "antikillerxxl@gmail.com"
		}
  ],
  "require": {
    "php": "^8.4",
    "yiisoft/yii2": "^2.0.45",
    "beeline/yii2-circuit-breaker": "^1.0.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Beeline\\ResilientMutex\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Beeline\\ResilientMutex\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
